<div class="page-content">
    <div class="row">
        <div class="col-xs-4">
            <!-- PAGE CONTENT BEGINS -->
            <table class="table table-bordered table-hover no-margin-bottom">
                <tr>
                    <td>当前股票数量：</td>
                    <td>--</td>
                </tr>
                <tr>
                    <td>定期数量：@totaldq</td>
                    <td></td>
                </tr>
                <tr>
                    <td>活期数量：@totalhq</td>
                    <td></td>
                </tr>
                <tr>
                    <td>委托卖单数量：</td>
                    <td>--</td>
                </tr>
                <tr>
                    <td>现金数量：</td>
                    <td>@totalxj</td>
                </tr>
                <tr>
                    <td>分红总和：</td>
                    <td>--</td>
                </tr>
                <tr>
                    <td>委托买单数量：</td>
                    <td>--</td>
                </tr>
                <tr>
                    <td>用户数量：</td>
                    <td>@totaluser</td>
                </tr>
            </table>

            <!-- PAGE CONTENT ENDS -->
        </div><!-- /.col -->
    </div><!-- /.row -->
</div><!-- /.page-content -->
