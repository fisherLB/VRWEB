@{
    Layout = "~/Areas/AdminCenter/Views/Shared/_Layout.cshtml";

    decimal totaluser = 0;
    decimal totalhq = 0;
    decimal totaldq = 0;
    decimal totalbuy = 0;
    decimal totalsell = 0;
    decimal totaldh = 0;
    decimal totalxj = 0;

    var userlist = MvcCore.Unity.Get<JN.Data.Service.IUserService>().List();
    totaluser = userlist.Count();

    totalxj = userlist.Count() > 0 ? userlist.Sum(x => x.Wallet2002) : 0;

    totalhq = userlist.Count() > 0 ? userlist.Sum(x => x.Wallet2003) : 0;
    var dqlist = MvcCore.Unity.Get<JN.Data.Service.ITimeDepositService>().List(x => x.Status < 2);
    totaldq = dqlist.Count() > 0 ? dqlist.Sum(x => x.DepositAmount) : 0;

}
<!-- page specific plugin styles -->
<link href="/Plugin/kindeditor-4.1.9/themes/default/default.css" rel="stylesheet" />
