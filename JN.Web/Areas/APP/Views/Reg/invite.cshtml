<script>
    $(function () {
        $("#submit").click(function () {
            $.post('/App/reg/index', $('#form').serialize(), function (d) {
                if (d.Status == 200) {
                    layer.msg("邀请成功！", {
                        icon: 1
                    });
                  location.href="/App/login"
                } else {
                    layer.msg(d.Message, {
                        icon: 2
                    });

                }
            }, 'json')

        })
    })


    //验证手机
    function codephone(thisval) {
        var phone = $("#phone").val();
        //var myreg = /^(((13[0-9]{1})|(18[0-9]{1})|(15[0-9]{1})|(17[0-9]{1}))+\d{8})$/;
        //if (!myreg.test(phone)) {
        //    layer.msg('请输入有效的手机号码', { icon: 2 });
        //    $("#phone").focus();
        //    return false;
        //}
        if (phone != "") {
            layer.msg('正在发送短信，请稍后...');
            $.post("/App/reg/SendMobileMsm", { myphone: phone }, function (data) {
                if (data.Status == 200) {
                    layer.msg('短信已发送', { icon: 1 });
                    time(thisval);
                } else {

                    layer.msg(data.Message, { icon: 2 });
                }
            });
        } else {
            layer.msg('手机不能为空', { icon: 2 });

            $("#phone").focus();
            return false;
        }
    }

    var yzmbtn = document.getElementById("yzm");
    if (yzmbtn != null) {
        var wait = 60;
        document.getElementById("yzm").disabled = false;
        function time(o) {
            if (wait == 0) {
                o.removeAttribute("disabled");
                o.innerHTML = "重新获取验证码";
                wait = 60;
            } else {
                o.setAttribute("disabled", true);
                o.innerHTML = "重新发送(" + wait + ")";
                wait--;
                setTimeout(function () {
                    time(o)
                },
                1000)
            }
        }
        document.getElementById("yzm").onclick = function () {
            codephone(this);
        }
    }


